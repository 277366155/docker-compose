init_worker_by_lua_file /usr/local/openresty/nginx/lua/redisfilter.lua;

upstream star-api-net{
    server 128.0.202.105:1013   weight=100; 
}
upstream star-api-java{
    server 128.0.255.96:12402   weight=100;
}

server{
    listen 80;
    server_name test-api-star-local.td365.com.cn;

    location /redis {
        default_type text/html;
        lua_code_cache on;
        content_by_lua_file 
    }

    # location ^~ /td-cloud/operation-service/test {

    # location ~* ^/td-cloud\/operation-service\/test {
    #     default_type text/html;
    #     charset utf-8;
    #     content_by_lua '
    #         ngx.say(ngx.var.uri)
    #         ngx.say("<br/>中文<br/>")
    #         ngx.say(split_path(ngx.var.uri))
    #         ngx.say("<br/>")
    #         ngx.say(ngx.var.args)
    #     ';
    # }

     # 匹配以/td-cloud/operation-service/开头的请求转向http://128.0.255.96:12402/页面。例如：
     # http://test-api-star-local.td365.com.cn/td-cloud/operation-service/ping.html 实际请求的是http://128.0.255.96:12402/ping.html
    location /td-cloud/operation-service/{
         proxy_pass http://star-api-java/;
    }

    # # 匹配以/td-cloud/operation-service/开头(不区分大小写)的请求转向http://128.0.255.96:12402/td-cloud/operation-service页面。例如：
    # # http://test-api-star-local.td365.com.cn/td-cloud/operation-service/ping.html 实际请求的是http://128.0.255.96:12402/td-cloud/operation-service/ping.html
    # location ~* ^\/td-cloud\/operation-service\/ {
    #     default_type text/html;
    #     charset utf-8;       
    #     proxy_pass http://star-api-java;
    #     #设置主机头和客户端真实地址，以便服务器获取客户端真实IP
    #     proxy_set_header Host $host;
    #     proxy_set_header X-Real-IP $remote_addr;
    #     proxy_set_header X-Forwarded-For $proxy_add_x_forwarded_for;
    # }




    location / {       
        default_type text/html;

        set_by_lua_file $backend conf/lua/set_upstream_by_uri.lua star-api-net star-api-java;
        #其中jq_one对应着upstream设置的集群名称
        proxy_pass http://$backend;

        #设置主机头和客户端真实地址，以便服务器获取客户端真实IP
        proxy_set_header Host $host;
        proxy_set_header X-Real-IP $remote_addr;
        proxy_set_header X-Forwarded-For $proxy_add_x_forwarded_for;
    }


}
